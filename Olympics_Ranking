#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--) {
	    int a,b,c,m,n,o;
	    cin>>a>>b>>c>>m>>n>>o;
	    int sum1=a+b+c;
	    int sum2=m+n+o;
	    if(sum1>sum2) {
	        cout<<"1"<<endl;
	    }
	    else {
	        cout<<"2"<<endl;
	    }
	}
	return 0;
}
